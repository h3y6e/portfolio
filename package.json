{
  "name": "portfolio",
  "private": true,
  "description": "heyhoe's portfolio",
  "author": "heyhoe",
  "scripts": {
    "dev": "gatsby develop",
    "clean": "gatsby clean && rimraf .lighthouseci",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "start": "run-s clean build serve",
    "type-check": "tsc --pretty",
    "format": "prettier --write '**/*.{js,ts,tsx,json,yml,md}' --ignore-path .gitignore",
    "lint": "eslint . --ext .js,.ts,.tsx --ignore-path .gitignore",
    "fix": "yarn lint --fix",
    "lh": "lhci autorun",
    "vrt": "percy exec -- mocha test/ --exit",
    "vrt:start": "start-server-and-test :9000 vrt"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/fira-code": "^4.5.13",
    "@fontsource/montserrat": "^5.0.3",
    "@fontsource/noto-sans-jp": "^5.0.11",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@lhci/cli": "^0.11.0",
    "bulma": "^0.9.4",
    "bulma-timeline": "^3.0.5",
    "gatsby": "^5.10.0",
    "gatsby-plugin-emotion": "^8.11.0",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-offline": "^6.11.0",
    "gatsby-plugin-sass": "^6.11.0",
    "gatsby-plugin-scroll-reveal": "^0.0.7",
    "gatsby-plugin-sharp": "^5.9.0",
    "gatsby-plugin-smoothscroll": "^1.2.0",
    "gatsby-plugin-typescript": "^5.9.0",
    "gatsby-source-filesystem": "^5.6.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "gatsby-transformer-yaml": "^5.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.66.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@percy/cli": "^1.20.0",
    "@percy/puppeteer": "^2.0.2",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.57.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "puppeteer": "^20.4.0",
    "rimraf": "^5.0.1",
    "scroll-to-bottomjs": "^1.1.0",
    "start-server-and-test": "^1.15.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h3y6e/portfolio"
  }
}

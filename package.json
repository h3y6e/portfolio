{
  "name": "portfolio",
  "private": true,
  "description": "heyhoe's portfolio",
  "author": "heyhoe",
  "scripts": {
    "dev": "gatsby develop",
    "clean": "gatsby clean && rimraf .lighthouseci",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "start": "run-s clean build serve",
    "type-check": "tsc --pretty",
    "format": "prettier --write '**/*.{js,ts,tsx,json,yml,md}' --ignore-path .gitignore",
    "lint": "eslint . --ext .js,.ts,.tsx --ignore-path .gitignore",
    "fix": "yarn lint --fix",
    "lh": "lhci autorun",
    "vrt": "percy exec -- mocha test/ --exit",
    "vrt:start": "start-server-and-test :9000 vrt"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.5",
    "@fontsource/fira-code": "^4.5.12",
    "@fontsource/montserrat": "^4.5.14",
    "@fontsource/noto-sans-jp": "^4.5.12",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@lhci/cli": "^0.11.0",
    "bulma": "^0.9.4",
    "bulma-timeline": "^3.0.5",
    "gatsby": "^5.4.2",
    "gatsby-plugin-emotion": "^8.6.0",
    "gatsby-plugin-image": "^3.3.2",
    "gatsby-plugin-manifest": "^5.7.0",
    "gatsby-plugin-offline": "^6.3.1",
    "gatsby-plugin-sass": "^6.8.0",
    "gatsby-plugin-scroll-reveal": "^0.0.7",
    "gatsby-plugin-sharp": "^5.8.1",
    "gatsby-plugin-smoothscroll": "^1.2.0",
    "gatsby-plugin-typescript": "^5.7.0",
    "gatsby-source-filesystem": "^5.6.0",
    "gatsby-transformer-sharp": "^5.5.0",
    "gatsby-transformer-yaml": "^5.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.58.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@percy/cli": "^1.20.0",
    "@percy/puppeteer": "^2.0.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "puppeteer": "^19.6.3",
    "rimraf": "^3.0.2",
    "scroll-to-bottomjs": "^1.1.0",
    "start-server-and-test": "^1.15.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h3y6e/portfolio"
  }
}

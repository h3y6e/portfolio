{
  "name": "portfolio",
  "private": true,
  "description": "heyhoe's portfolio",
  "author": "heyhoe",
  "scripts": {
    "dev": "gatsby develop",
    "clean": "gatsby clean && rimraf .lighthouseci",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "start": "run-s clean build serve",
    "type-check": "tsc --pretty",
    "format": "prettier --write '**/*.{js,ts,tsx,json,yml,md}' --ignore-path .gitignore",
    "lint": "eslint . --ext .js,.ts,.tsx --ignore-path .gitignore",
    "fix": "yarn lint --fix",
    "lh": "lhci autorun",
    "vrt": "percy exec -- mocha test/ --exit",
    "vrt:start": "start-server-and-test :9000 vrt"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@fontsource/fira-code": "4.5.11",
    "@fontsource/montserrat": "4.5.12",
    "@fontsource/noto-sans-jp": "4.5.11",
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-brands-svg-icons": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@lhci/cli": "0.9.0",
    "bulma": "0.9.4",
    "bulma-timeline": "3.0.5",
    "gatsby": "3.14.6",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-emotion": "6.14.0",
    "gatsby-plugin-manifest": "3.14.0",
    "gatsby-plugin-offline": "4.14.0",
    "gatsby-plugin-react-helmet": "4.14.0",
    "gatsby-plugin-sass": "4.14.0",
    "gatsby-plugin-scroll-reveal": "0.0.7",
    "gatsby-plugin-sharp": "3.14.3",
    "gatsby-plugin-smoothscroll": "1.2.0",
    "gatsby-plugin-typescript": "3.14.0",
    "gatsby-source-filesystem": "3.14.0",
    "gatsby-transformer-sharp": "3.14.0",
    "gatsby-transformer-yaml": "3.14.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "sass": "1.54.9",
    "typescript": "4.8.3"
  },
  "devDependencies": {
    "@percy/cli": "^1.10.3",
    "@percy/puppeteer": "^2.0.2",
    "@types/node": "18.7.21",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/react-helmet": "6.1.5",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "mocha": "^10.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "puppeteer": "^17.1.3",
    "rimraf": "3.0.2",
    "scroll-to-bottomjs": "^1.1.0",
    "start-server-and-test": "^1.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h3y6e/portfolio"
  },

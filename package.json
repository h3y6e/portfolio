    "gatsby-source-filesystem": "2.11.1",
    "gatsby-transformer-sharp": "2.12.0",
    "gatsby-transformer-yaml": "2.11.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
